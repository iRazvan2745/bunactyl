    "dev": "bun --watch src/index.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "pterodactyl",
    "sdk",
    "api",
    "typescript",
    "bun",
    "bunactyl"
  ],
  "author": "irazz",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "bunactyl": "^1.0.1"
  }
}
